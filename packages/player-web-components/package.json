{
  "name": "@tidal-music/player-web-components",
  "version": "0.0.3",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/tidal-music/tidal-sdk-web"
  },
  "license": "UNLICENSED",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "tidal-music",
    "web-components"
  ],
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build -m development",
    "clean": "rm -rf coverage dist .eslintcache",
    "dev": "vite --debug --cors -c=./vite.config.ts",
    "lint": "eslint . --cache --cache-strategy content",
    "lint:ci": "eslint . --quiet",
    "lint:fix": "pnpm run lint --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "types": "tsc",
    "prepublishOnly": "npx --yes @skypack/package-check --cwd ."
  },
  "devDependencies": {
    "@tidal-music/auth": "workspace:^",
    "@tidal-music/common": "workspace:^",
    "@tidal-music/player": "workspace:^",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.7",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-plugin-dts": "3.7.1",
    "vitest": "0.34.6"
  }
}
