  "keywords": [
    "tidal",
    "player"
  ],
  "contributors": [
    {
      "name": "Jeremy Karlsson",
      "email": "jeremyk@tidal.com",
      "url": "https://tidal.com"
    }
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
