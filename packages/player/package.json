{
  "name": "@tidal-music/player",
  "version": "0.1.2",
  "description": "Player logic for TIDAL",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "wtr": "web-test-runner",
    "test": "bash ./scripts/test.sh",
